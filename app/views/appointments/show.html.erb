<%= link_to @appointment.doctor.name, doctor_path(@appointment.doctor) %>
<%= link_to @appointment.patient.name, patient_path(@appointment.patient) %>
<%= @appointment.appointment_datetime.strftime("%B").to_s + " " + @appointment.appointment_datetime.day.to_s + ", " + @appointment.appointment_datetime.year.to_s + " " + @appointment.appointment_datetime.hour.to_s + ":" + @appointment.appointment_datetime.min.to_s + "0"%>
