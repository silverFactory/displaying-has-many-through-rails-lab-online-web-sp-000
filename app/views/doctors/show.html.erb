<% @doctor.appointments.each do |a| %>
<%= link_to a.patient.name, patient_path(a.patient) %>
<%= a.appointment_datetime.strftime("%B").to_s + " " + a.appointment_datetime.day.to_s + ", " + a.appointment_datetime.year.to_s + " at " + a.appointment_datetime.hour.to_s + ":" + a.appointment_datetime.min.to_s%>
<% end %>
